<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ecea.resonance15" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Resonance_Home"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ListActivity"
            android:label="@string/title_activity_list"
            android:parentActivityName=".Resonance_Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ecea.resonance15.Resonance_Home" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EventLayout"
            android:label="@string/title_activity_event_layout"
            android:parentActivityName=".ListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ecea.resonance15.ListActivity" />
        </activity>
    </application>

</manifest>
